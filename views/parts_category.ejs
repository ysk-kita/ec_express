<div class="d-flex flex-column justify-content-start border">
  <div class="d-flex justify-content-center sm-category">
    <a class="sm-margin-0" href="/category">商品一覧</a>
  </div>
  <div class="d-flex justify-content-start">
    <button type="button" class="sm-category-btn" data-toggle="collapse" data-target="#category-list" aria-expanded="false" aria-controls="category-list" id="category-btn">
      <span id="category-btn-hide">カテゴリ一覧</span>
    </button>
  </div>
  <div class="collapse" id="category-list">
    <% var i=1 %>
    <% for (var cate of categoryList) { %>
      <% if(i==1) { %>
      <div class="d-flex justify-content-center sm-category-start">
      <% } else if(i==categoryList.length) { %>
      <div class="d-flex justify-content-center sm-category-end">
      <% } else { %>
      <div class="d-flex justify-content-center sm-category">
      <% } %>
        <a class="sm-margin-0" href="/category?cate=<%= cate.id %>"><%= cate.title %></a>
      </div>
      <% i++; %>
    <% } %>
  </div>
</div>
